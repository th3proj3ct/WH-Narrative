        <selectionEntry type="upgrade" import="true" name="Decurion Lanius" hidden="false" id="2b8e-cde3-82fc-359a">
          <entryLinks>
            <entryLink import="true" name="Banestrike Bolt Cannon" hidden="true" id="7d09-cd03-f758-9a4d" type="selectionEntry" targetId="9765-c11a-338d-7d5a">
